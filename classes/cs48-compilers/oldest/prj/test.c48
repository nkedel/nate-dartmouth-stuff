/* example function */
/* fib(to_fib) - calculated fibonacci numbers iteratively */
int fib(int to_fib) {
    int i, fibbed, last, penult;   /* loop, total, previous 2 numbers */
    if (fib < 1) { return(0); }
    penult = 0; fibbed = 0; last = 1;
    for (i=1; i <= fib;++i) {
        fibbed = last + penult; penult = last; last = fibbed;
	}
    return(fibbed);
    }

/* example main function */
